<h1>Create a New Heroine</h1>
<%=  form_for @heroine do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name %><br>
  <%= f.label :super_name  %>
  <%= f.text_field :super_name %><br>
  Choose a power:
  <%=  f.select :power_id, Power.all.collect { |p| [ p.name, p.id ] }%><br>
  <%= f.submit %>
<% end %><br>

<%= link_to "Cancel", heroines_path %>
