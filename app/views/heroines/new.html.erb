<h1>Create a New Heroine!</h1>
<br>

<%= form_for @heroine do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %><br><br>
  <%= f.label :power_id %>
  <%= f.collection_select :power_id, Power.all, :id, :name %><br><br>
  <%= f.submit %>



<% end %>
