<h2>All Heroines</h2>


 <%= form_tag("/heroine", method: "get") do %>
  <%= label_tag(:q, "Search by Power:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
    <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>


<%= link_to "Create New Heroine", new_heroine_path %>
